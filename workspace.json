{
	"version": 2,
	"projects": {
		"client": "apps/client",
		"client-e2e": "apps/client-e2e",
		"ui": "libs/ui"
	},
	"cli": {
		"analytics": false,
		"defaultCollection": "@nrwl/angular"
	},
	"generators": {
		"@nrwl/angular": {
			"application": {
				"linter": "eslint"
			},
			"library": {
				"linter": "eslint"
			},
			"storybook-configuration": {
				"linter": "eslint"
			}
		},
		"@nrwl/angular:application": {
			"style": "scss",
			"linter": "eslint",
			"unitTestRunner": "jest",
			"e2eTestRunner": "cypress"
		},
		"@nrwl/angular:library": {
			"style": "scss",
			"linter": "eslint",
			"unitTestRunner": "jest"
		},
		"@nrwl/angular:component": {
			"style": "scss"
		}
	},
	"defaultProject": "client"
}
